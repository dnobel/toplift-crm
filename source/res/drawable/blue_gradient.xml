<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/highrise_blue" />

            <gradient
                android:angle="270"
                android:endColor="@color/highrise_blue"
                android:startColor="@color/highrise_blue" />
        </shape>
    </item>
    <item
        android:bottom="-1dp"
        android:left="-1dp"
        android:right="-1dp"
        android:top="0dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="#1A719C" />

            <solid android:color="#00000000" />

            <corners android:radius="0dp" />
        </shape>
    </item>
    <item
        android:bottom="0dp"
        android:left="-1dp"
        android:right="-1dp"
        android:top="-2dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="2dp"
                android:color="#00496E" />

            <solid android:color="#00000000" />

            <corners android:radius="0dp" />
        </shape>
    </item>

</layer-list>